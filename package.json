{
  "name": "@perfect-schema/extends",
  "version": "1.0.0",
  "description": "Extends PerfectSchema instances from other schemas.",
  "author": "Yanick Rochon <yanick.rochon@gmail.com>",
  "license": "MIT",
  "main": "src/extends.js",
  "browser": "dist/perfect-schema-extends.min.js",
  "engines": {
    "node": ">= 8.2.1"
  },
  "scripts": {
